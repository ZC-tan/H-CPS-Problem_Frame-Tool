  <interfaces targetDomain="//@domain.1" sourceDomain="//@domain.6"/>
  <interfaces targetDomain="//@domain.0" sourceDomain="//@domain.7" beginControlSharedPhenomena="" endControlSharedPhenomena="显示状态等图标"/>
  <interfaces targetDomain="//@domain.5" sourceDomain="//@domain.7"/>
  <interfaces targetDomain="//@domain.5" sourceDomain="//@domain.2"/>
  <interfaces targetDomain="//@domain.5" sourceDomain="//@domain.3"/>
  <reference xsi:type="ProblemFrame:RequirementReference" source="//@requirement.0" reqReference="//@domain.1"/>
  <reference xsi:type="ProblemFrame:RequirementReference" source="//@requirement.0" reqReference="//@domain.4"/>
  <reference xsi:type="ProblemFrame:ConstrainingRequirementReference" source="//@requirement.0" constrains="//@domain.3"/>
</ProblemFrame:ProblemDiagram>
